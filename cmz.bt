struct HEADER {
    char fileID[4];
    int compressedSize;
    int uncompressedSize;
    int checkSum;
    ubyte filenameLen;
    byte unknown[3];
    char filename[filenameLen];
} headerInfo <bgcolor=cLtBlue>;

local int ds = FileSize() - FTell();
struct DATA {
    uchar dclImplodedData[ds];
    
} headerInfo <bgcolor=cLtYellow>;